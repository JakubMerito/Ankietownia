{% extends 'base.html.twig' %}

{% block title %}Moje konto{% endblock %}

{% block body %}
    <h1>Edytuj dane konta</h1>

    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    <button class="btn btn-primary">Zapisz zmiany</button>
    {{ form_end(form) }}
{% endblock %}
